{
    "manifest_version": 2,
    "name": "Ramoun Launcher",
    "description": "a launcher for all of Ramoun's Social Media",
    "version": "1.0.0",
    "icons": {"128": "images/logo_128.png"},
    "browser_action": {
        "default_icon": "images/logo.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab"]
}